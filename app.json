        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e87200"
      },
      
      "package": "off.camp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-google-signin/google-signin"]
  }
